{
	"name": "screencast-helper",
	"displayName": "Screencast Helper",
	"description": "For practice",
	"publisher": "pkishoez",
	"version": "0.0.2",
	"repository": "https://www.github.com/pkishoez/vscode-screencast-helper",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "screencastHelper.createProject",
				"title": "Add",
				"icon": "$(plus)"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "screencastHelper",
					"name": "Screencase Helper"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "screencastHelper",
				"contents": "No Screencasts recorded! Record one to show up here."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/lodash": "^4.14.155",
		"@types/uuid": "^8.0.0",
		"lodash": "^4.17.15",
		"uuid": "^8.1.0",
		"xstate": "^4.10.0"
	}
}
